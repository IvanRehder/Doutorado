@CONFERENCE{Dobbins2015103,
author={Dobbins, T. and Hill, J. and Thompson, T. and McCartan, S. and Brand, T. and Smoker, A.},
title={Human-centred, scalable, combat system design for littoral operations},
journal={RINA, Royal Institution of Naval Architects - Warship 2015: Future Surface Vessels, Papers},
year={2015},
pages={103-113},
note={cited By 1; Conference of International Conference on Warship 2015: Future Surface Vessels ; Conference Date: 10 June 2015 Through 11 June 2015;  Conference Code:115820},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84947080788&partnerID=40&md5=249b4cbf9785877177140b1072b2cc8a},
affiliation={STResearch, United Kingdom; Chalmers University, Sweden; Trident Marine, United Kingdom; T-Design, Coventry, United Kingdom; EBDIG-IRC, Department of Industrial Design, Coventry University, United Kingdom; Strongwake, Canada; Lund University, Sweden},
abstract={Combat operations in the littoral and amphibious environments present unique challenges to both the warfighters, and the system designers. High-tempo navigation, in shallow / congested waters, and in degraded visual environments, increases the stresses on the individuals and teams before combat related tasks are considered. All combat tasks require the crew, and the crafts systems, to have the appropriate level of situation awareness and decision making capability. This Joint Cognitive System (JCS) supports the interoperability needed to achieve successful operational outcomes, particularly in joint operations. Resilience, is essential in the high-tempo coastal environment. Littoral / amphibious craft, being typically small and fast, and by necessity lean manned must be efficient and optimised. Understanding the crew's roles, tasks and competences provides the definitions required for the information architectures and flow, both intra and inter vessel, that supports the JCS and defines the vessel's Combat System. The JCS, and therefore the Combat System, can be scaled with the vessels size, as a number of the crews / systems functions remain the same. Designing the combat JCS from a human-centred perspective, provides an essential foundation for designing the vessel and delivering the required littoral / amphibious capability. © 2015: The Royal Institution of Naval Architects.},
keywords={Cognitive systems;  Decision making;  Naval vessels;  Systems analysis;  Warships, Coastal environments;  Combat operations;  Degraded visual environments;  Information architectures;  Joint cognitive systems;  Joint operations;  Littoral operations;  Situation awareness, Interoperability},
