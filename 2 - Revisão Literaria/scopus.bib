@CONFERENCE{Imhof20175465,
author={Imhof, B. and Hogle, M. and Davenport, B. and Weiss, P. and Urbina, D. and Røyrvik, J. and Vögele, T. and Parro, V. and Nottle, A.},
title={Project Moonwalk: Lessons learnt from testing human robot collaboration scenarios in a lunar and Martian simulation},
