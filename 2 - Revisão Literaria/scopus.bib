@CONFERENCE{Beckers2019,
author={Beckers, G. and Sijs, J. and Van Diggelen, J. and Van Dijk, R.J.E. and Bouma, H. and Lomme, M. and Hommes, R. and Hillerstrom, F. and Van Der Waa, J. and Van Velsen, A. and Mannucci, T. and Voogd, J. and Van Staal, W. and Veltman, K. and Wessels, P. and Huizing, A.},
title={Intelligent autonomous vehicles with an extendable knowledge base under meaningful human control},
