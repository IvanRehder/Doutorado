@ARTICLE{Pransky2022177,
author={Pransky, J.},
title={The Pransky interview: Dr Raffaello D’Andrea, Founder, CEO, and Chairman of the board at Verity; Entrepreneur; Professor; Scientist and Artist},
journal={Industrial Robot},
year={2022},
volume={49},
